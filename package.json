    "merge:worklets": "echo \"hey\""
  },
  "devDependencies": {
    "@types/audioworklet": "^0.0.33",
    "@types/emscripten": "^1.39.6",
    "cross-env": "^7.0.3",
    "ts-loader": "^9.4.1",
